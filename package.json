{
  "name": "@sh20raj/sheet-flow",
  "version": "1.0.0",
  "description": "Transform Google Sheets into powerful, customizable REST APIs with real-time sync and advanced features",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "docs": "typedoc"
  },
  "keywords": [
    "google-sheets",
    "api",
    "database",
    "spreadsheet",
    "rest-api",
    "typescript",
    "real-time",
    "orm"
  ],
  "author": "sh20raj",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "google-spreadsheet": "^4.1.1",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "google-auth-library": "^9.4.1",
    "cache-manager": "^5.3.1",
    "http-status-codes": "^2.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SH20RAJ/SheetFlow"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "directories": {
    "example": "examples"
  }
}
